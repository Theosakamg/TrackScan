<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="2.5dp"
    android:paddingLeft="10dp"
    android:paddingRight="10dp"
    android:paddingTop="2.5dp" >
	    <TextView
			android:id="@+id/row_orderprod_login"
	    	android:layout_width="match_parent"
	    	android:layout_height="wrap_content"
	        android:layout_alignParentTop="true" />
	    <TextView
			android:id="@+id/row_orderprod_producttype"
	    	android:layout_width="match_parent"
	    	android:layout_height="wrap_content"
			android:layout_below="@+id/row_orderprod_login" />
	    <TextView
			android:id="@+id/row_orderprod_materialtype"
	    	android:layout_width="match_parent"
	    	android:layout_height="wrap_content"
			android:layout_below="@+id/row_orderprod_producttype" />
	    <TextView
			android:id="@+id/row_orderprod_quantity"
	    	android:layout_width="match_parent"
	    	android:layout_height="wrap_content"
			android:layout_below="@+id/row_orderprod_materialtype" />

</RelativeLayout>
