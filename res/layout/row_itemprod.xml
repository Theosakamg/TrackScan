<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="2.5dp"
    android:paddingLeft="10dp"
    android:paddingRight="10dp"
    android:paddingTop="2.5dp" >
	    <TextView
			android:id="@+id/row_itemprod_name"
	    	android:layout_width="match_parent"
	    	android:layout_height="wrap_content"
	        android:layout_alignParentTop="true" />
	    <TextView
			android:id="@+id/row_itemprod_state"
	    	android:layout_width="match_parent"
	    	android:layout_height="wrap_content"
			android:layout_below="@+id/row_itemprod_name" />
	    <TextView
			android:id="@+id/row_itemprod_updatedate"
	    	android:layout_width="match_parent"
	    	android:layout_height="wrap_content"
			android:layout_below="@+id/row_itemprod_state" />
	    <TextView
			android:id="@+id/row_itemprod_ordercustomer"
	    	android:layout_width="match_parent"
	    	android:layout_height="wrap_content"
			android:layout_below="@+id/row_itemprod_updatedate" />
	    <TextView
			android:id="@+id/row_itemprod_currentzone"
	    	android:layout_width="match_parent"
	    	android:layout_height="wrap_content"
			android:layout_below="@+id/row_itemprod_ordercustomer" />

</RelativeLayout>
